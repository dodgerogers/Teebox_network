<h1>Tags</h1>

<%= render partial: "tags/info" %>

<%= render template: "tags/new" %>

<h2>Search Tags</h2>

<%= form_tag tags_path, method: "get" do %>
<%= text_field_tag :search, params[:search], id: "tag-search", placeholder: "Search" %>
<% end %>

<div id="tags">
<%= render @tags %>
</div>

<%= will_paginate @tags %>