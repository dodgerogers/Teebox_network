<div class="row">
	<div id="tag_cloud" class="col-md-12">
		<% questions.each do |tag| %>
			<div class="tag_cloud_tag">
				<%= link_to "#{tag.name} x #{tag.tag_count}", tagged_path(tag.name), class:  tag_class(tag) %>
			</div>
		<% end %>
		<div class="tag_cloud_tag">
			<%= link_to "View all Tags", tags_path, class: "tag" %>
		</div>	
	</div>
</div>
