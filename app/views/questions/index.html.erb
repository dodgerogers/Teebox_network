<%= render template: "shared/teebox_banner" %>

<%= render partial: "shared/nav_links", locals: {decorator: @decorator} %>

<% if params[:search] %> 
<%= render partial: "questions/questions", locals: {questions: @decorator.questions} %>
<% elsif params[:tag] %>
<%= render partial: "questions/questions", locals: {questions: @decorator.tagged_questions} %>
<% else  %>
<%= render partial: "questions/questions", locals: {questions: @decorator.newest_questions} %>
<% end %>