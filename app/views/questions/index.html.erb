<%= render partial: "questions/tag_cloud", locals: {tags: @tags} %>
	
<% if params[:search] || params[:tag] %> 
<%= render partial: "questions/questions", locals: {questions: @questions} %>
<% else %>
<%= render partial: "questions/tabbed_index", locals: {questions: @questions, unanswered: @unanswered, votes: @votes} %>
<% end %>
	

