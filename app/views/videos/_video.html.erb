<div id='video_<%="#{video.id}" %>' class="videos">
  <%= link_to image_tag('video_screen.jpg'), video %>
	<h4><%= link_to File.basename(video.file).titleize, video %></h4>
	 <%= link_to "Delete", video_path(video), method: :delete, remote: true %> | Uploaded <%= time_ago_in_words(video.created_at)  %> ago
</div>
