<% if @video.new_record? %>
	alert("Failed to upload Video: <%= j @video.errors.full_messages.join(', ').html_safe %>");
<% else %>
var video = $("<%= j render(@video) %>").hide();
$('#videos').prepend(video);
video.fadeIn(1500);
$('#video_<%="#{@video.id}" %>')
$('.well').html("").append('Video Uploaded âœ“').fadeIn();
$('#dropzone').fadeOut(1200);
<% end %>